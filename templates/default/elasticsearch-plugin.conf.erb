<LoadPlugin "python">
    Globals true
</LoadPlugin>

<% if not @typesdb.empty? %>
<%= "TypesDB \"#{@typesdb}\"" %>
<% end %>

<% if not @config.empty? %>
<Plugin "python">
  ModulePath <%= collectd_option(@config["ModulePath"]) %>
  Import <%= collectd_option(@config["Import"]) %>
  <%= "<Module \"#{@name}\">" %>
    <% @module_config.each_pair do |key, value|
      if value.is_a? Array
        value.each do |subvalue| %>
    <%= key %> <%= collectd_option(subvalue) %>
    <% end else %>
    <%= key %> <%= collectd_option(value) %>
    <% end end %>
  </Module>
</Plugin>
<% end %>
